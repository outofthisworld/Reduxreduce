{
  "name": "reduxreduce",
  "version": "1.0.0",
  "description": "Utilities for working with redux",
  "main": "index.js",
  "scripts": {
    "rollup": "rollup -c && babel index.js --out-file index.js",
    "minify": "minify index.js -d minified",
    "rename_min": "mv minified/index.js minified/reduxreduce.js",
    "copy_babel_output": "cp index.js reduxreduce.js",
    "copy_minified": "mv minified/reduxreduce.js reduxreduce.min.js",
    "build":
      "npm run rollup && npm run minify && npm run rename_min && npm run copy_babel_output && npm run copy_minified",
    "clean":
      "rm -rf minified && rm reduxreduce.js && rm reduxreduce.min.js && rm index.js",
    "test": "mocha"
  },
  "keywords": [
    "redux",
    "utils",
    "utility",
    "library",
    "reducer",
    "reduxreducer",
    "composition",
    "react",
    "store",
    "dispatch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/outofthisworld/Reduxreduce"
  },
  "bugs": {
    "url": "https://github.com/outofthisworld/Reduxreduce/issues"
  },
  "author": "outofthisworld",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier-eslint": "^8.8.1"
  }
}
